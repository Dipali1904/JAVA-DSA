class Solution {
    public int[] twoSum(int[] nums, int target) {
     int[] newarr = new int[2];
     HashMap<Integer,Integer> mpp = new HashMap<>();
     for(int i=0;i<nums.length;i++){
        int key = target-nums[i];
        if(mpp.containsKey(key)){
            newarr[0] = i;
            newarr[1] = mpp.get(key);
            return newarr;
        }
        mpp.put(nums[i],i);
     }
     return newarr;   
    }
}
